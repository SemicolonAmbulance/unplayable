  <rect x="60" y="80" width="4" height="40" fill="url(#audioGradient)"/>
  <rect x="70" y="60" width="4" height="80" fill="url(#audioGradient)"/>
  <rect x="80" y="85" width="4" height="30" fill="url(#audioGradient)"/>
  <rect x="90" y="75" width="4" height="50" fill="url(#audioGradient)"/>
  <rect x="100" y="65" width="4" height="70" fill="url(#audioGradient)"/>
  <rect x="110" y="75" width="4" height="50" fill="url(#audioGradient)"/>
  <rect x="120" y="85" width="4" height="30" fill="url(#audioGradient)"/>
  <rect x="130" y="70" width="4" height="60" fill="url(#audioGradient)"/>
  <rect x="140" y="80" width="4" height="40" fill="url(#audioGradient)"/>
  <rect x="150" y="90" width="4" height="20" fill="url(#audioGradient)"/>
  
